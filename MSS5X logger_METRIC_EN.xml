                        <conversion units="bar" expr="(x*3+500)/1000" format="0.000" gauge_min="0" gauge_max="1.5" gauge_step="0.1" />
                        <conversion units="inHg" expr="(x*3+500)/33.864" format="0.00" gauge_min="-40" gauge_max="60" gauge_step="10" />
